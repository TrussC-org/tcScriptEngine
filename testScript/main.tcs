{
  "files": {
    "main": "// Comprehensive TrussSketch Test Suite\n// Press 1-0 to switch samples\n\nint currentSample = 1;\n\nvoid setup() {\n    logNotice(\"=== TrussSketch Test Suite ===\");\n    logNotice(\"Press 1-0 to switch samples\");\n    setCircleResolution(64);\n    setupSample1();\n    setupSample2();\n    setupSample3();\n    setupSample4();\n    setupSample5();\n    setupSample6();\n    setupSample7();\n    setupSample8();\n    setupSample9();\n    setupSample10();\n}\n\nvoid draw() {\n    clear(0.12f);\n    if (currentSample == 1) drawSample1();\n    else if (currentSample == 2) drawSample2();\n    else if (currentSample == 3) drawSample3();\n    else if (currentSample == 4) drawSample4();\n    else if (currentSample == 5) drawSample5();\n    else if (currentSample == 6) drawSample6();\n    else if (currentSample == 7) drawSample7();\n    else if (currentSample == 8) drawSample8();\n    else if (currentSample == 9) drawSample9();\n    else if (currentSample == 10) drawSample10();\n    drawHeader();\n}\n\nvoid drawHeader() {\n    setColor(0.0f, 0.0f, 0.0f, 0.8f);\n    drawRect(0, 0, getWindowWidth(), 25);\n    setColor(1.0f, 1.0f, 1.0f);\n    string titles = \"1:Shapes 2:Transform 3:Types 4:Easing 5:Texture 6:Sound 7:Font 8:Input 9:Style 0:Math\";\n    drawBitmapString(titles, 10, 8);\n    for (int i = 0; i < 10; i++) {\n        float x = getWindowWidth() - 120 + float(i) * 12;\n        if (i == currentSample - 1) { setColor(0.3f, 0.9f, 1.0f); drawCircle(x, 12, 4); }\n        else { setColor(0.4f, 0.4f, 0.4f); drawCircle(x, 12, 2); }\n    }\n}\n\nvoid keyPressed(int key) {\n    if (key >= 49 && key <= 57) currentSample = key - 48;\n    else if (key == 48) currentSample = 10;\n    if (currentSample == 8) keySample8(key);\n}\n\nvoid mousePressed(float x, float y, int button) {\n    if (currentSample == 8) mouseSample8(x, y, button);\n}\n",

    "1_shapes": "// Sample 1: Basic Shapes\nfloat t1 = 0.0f;\n\nvoid setupSample1() { }\n\nvoid drawSample1() {\n    t1 += getDeltaTime();\n    setColor(1.0f, 1.0f, 1.0f, 0.6f);\n    drawBitmapString(\"Basic Shapes: rect, circle, ellipse, triangle, line, point\", 10, 40);\n\n    // Shapes row\n    setColor(0.9f, 0.3f, 0.3f);\n    drawRect(50, 80, 80, 50);\n    setColor(0.3f, 0.9f, 0.3f);\n    drawCircle(200, 105, 35);\n    setColor(0.3f, 0.3f, 0.9f);\n    drawEllipse(320, 105, 60, 35);\n    setColor(0.9f, 0.9f, 0.3f);\n    drawTriangle(420, 130, 470, 70, 520, 130);\n\n    // Lines\n    setColor(0.9f, 0.5f, 0.9f);\n    for (int i = 0; i < 8; i++) {\n        float y = sin(t1 * 2.0f + float(i) * 0.4f) * 20;\n        drawLine(50 + float(i) * 25, 180, 70 + float(i) * 25, 230 + y);\n    }\n\n    // Points spiral\n    setColor(1.0f, 0.8f, 0.2f);\n    for (int i = 0; i < 60; i++) {\n        float a = float(i) * 0.2f + t1;\n        float r = float(i) * 0.8f;\n        drawPoint(350 + cos(a) * r, 200 + sin(a) * r);\n    }\n\n    // Animated shapes\n    setColor(1.0f, 1.0f, 1.0f, 0.6f);\n    drawBitmapString(\"Animated:\", 10, 280);\n\n    float pulse = 30 + sin(t1 * 3.0f) * 15;\n    setColor(0.5f + sin(t1) * 0.3f, 0.7f, 0.9f);\n    drawCircle(100, 350, pulse);\n\n    for (int i = 0; i < 10; i++) {\n        float a = float(i) * TAU / 10.0f + t1;\n        float cx = 280 + cos(a) * 50;\n        float cy = 350 + sin(a) * 50;\n        float hue = float(i) / 10.0f;\n        setColor(0.5f + 0.5f * sin(hue * TAU), 0.5f + 0.5f * sin(hue * TAU + 2.1f), 0.5f + 0.5f * sin(hue * TAU + 4.2f), 0.7f);\n        drawCircle(cx, cy, 12);\n    }\n\n    for (int i = 0; i < 20; i++) {\n        float cx = 450 + float(i) * 12;\n        float cy = 350 + sin(t1 * 2.0f + float(i) * 0.4f) * 40;\n        setColor(1.0f, 0.5f + float(i) / 40.0f, 0.2f);\n        drawCircle(cx, cy, 5);\n    }\n}\n",

    "2_transform": "// Sample 2: Transform\nfloat t2 = 0.0f;\n\nvoid setupSample2() { }\n\nvoid drawSample2() {\n    t2 += getDeltaTime();\n    setColor(1.0f, 1.0f, 1.0f, 0.6f);\n    drawBitmapString(\"Transform: translate, rotate, scale, push/popMatrix, resetMatrix\", 10, 40);\n\n    // Translate\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"translate\", 60, 70);\n    pushMatrix();\n    translate(100, 120);\n    setColor(0.9f, 0.4f, 0.4f);\n    drawRect(-25, -25, 50, 50);\n    translate(60, 0);\n    setColor(0.4f, 0.9f, 0.4f);\n    drawRect(-20, -20, 40, 40);\n    popMatrix();\n\n    // Rotate\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"rotate\", 260, 70);\n    pushMatrix();\n    translate(300, 120);\n    rotate(t2);\n    setColor(0.4f, 0.4f, 0.9f);\n    drawRect(-30, -30, 60, 60);\n    rotate(t2 * 2.0f);\n    translate(50, 0);\n    setColor(0.9f, 0.9f, 0.4f);\n    drawRect(-12, -12, 24, 24);\n    popMatrix();\n\n    // Scale\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"scale\", 440, 70);\n    pushMatrix();\n    translate(480, 120);\n    float s = 0.6f + sin(t2 * 2.0f) * 0.3f;\n    scale(s, s);\n    setColor(0.9f, 0.4f, 0.9f);\n    drawCircle(0, 0, 40);\n    popMatrix();\n\n    // Combined\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"combined\", 60, 200);\n    pushMatrix();\n    translate(120, 280);\n    rotate(t2 * 0.5f);\n    scale(1.0f + sin(t2) * 0.2f);\n    setColor(0.3f, 0.8f, 0.8f);\n    drawRect(-40, -40, 80, 80);\n    translate(70, 0);\n    rotate(-t2 * 2.0f);\n    setColor(0.8f, 0.3f, 0.8f);\n    drawRect(-15, -15, 30, 30);\n    popMatrix();\n\n    // Nested\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"nested (3 levels)\", 300, 200);\n    pushMatrix();\n    translate(380, 280);\n    setColor(0.9f, 0.3f, 0.3f, 0.4f);\n    drawCircle(0, 0, 60);\n    pushMatrix();\n    rotate(t2);\n    translate(40, 0);\n    setColor(0.3f, 0.9f, 0.3f, 0.6f);\n    drawCircle(0, 0, 25);\n    pushMatrix();\n    rotate(-t2 * 2.0f);\n    translate(18, 0);\n    setColor(0.3f, 0.3f, 0.9f);\n    drawCircle(0, 0, 10);\n    popMatrix();\n    popMatrix();\n    popMatrix();\n\n    // resetMatrix\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"resetMatrix\", 500, 200);\n    pushMatrix();\n    translate(550, 280);\n    rotate(t2);\n    setColor(0.9f, 0.6f, 0.2f);\n    drawCircle(0, 0, 25);\n    resetMatrix();\n    setColor(0.5f, 0.5f, 0.5f, 0.5f);\n    drawCircle(550, 280, 40);\n    popMatrix();\n}\n",

    "3_types": "// Sample 3: Types (Vec2, Vec3, Color, Rect)\nfloat t3 = 0.0f;\nVec2[] particles(15);\nVec2[] velocities(15);\n\nvoid setupSample3() {\n    for (int i = 0; i < 15; i++) {\n        particles[i] = Vec2(random(50, 280), random(80, 200));\n        velocities[i] = Vec2(random(-40, 40), random(-40, 40));\n    }\n}\n\nvoid drawSample3() {\n    t3 += getDeltaTime();\n    setColor(1.0f, 1.0f, 1.0f, 0.6f);\n    drawBitmapString(\"Types: Vec2, Vec3, Color, Rect - methods and operations\", 10, 40);\n\n    // Vec2 particles\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"Vec2 particles (distance, +, normalized)\", 10, 65);\n    for (int i = 0; i < 15; i++) {\n        particles[i] = particles[i] + velocities[i] * getDeltaTime();\n        if (particles[i].x < 50 || particles[i].x > 280) velocities[i].x *= -1;\n        if (particles[i].y < 80 || particles[i].y > 200) velocities[i].y *= -1;\n        Vec2 nv = velocities[i].normalized();\n        setColor(0.5f + nv.x * 0.5f, 0.5f + nv.y * 0.5f, 0.8f);\n        drawCircle(particles[i].x, particles[i].y, 4);\n    }\n    for (int i = 0; i < 15; i++) {\n        for (int j = i + 1; j < 15; j++) {\n            float d = particles[i].distance(particles[j]);\n            if (d < 60) {\n                setColor(0.5f, 0.8f, 1.0f, (1.0f - d / 60.0f) * 0.4f);\n                drawLine(particles[i].x, particles[i].y, particles[j].x, particles[j].y);\n            }\n        }\n    }\n\n    // Vec2_fromAngle\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"Vec2_fromAngle\", 320, 65);\n    for (int i = 0; i < 12; i++) {\n        float a = float(i) * TAU / 12.0f + t3;\n        Vec2 d = Vec2_fromAngle(a, 40.0f);\n        Vec2 p = Vec2(400, 140) + d;\n        setColor(0.9f, 0.6f, 0.2f);\n        drawLine(400, 140, p.x, p.y);\n        drawCircle(p.x, p.y, 4);\n    }\n\n    // Color lerp\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"Color.lerp\", 10, 230);\n    for (int i = 0; i < 15; i++) {\n        Color c1(1.0f, 0.2f, 0.2f);\n        Color c2(0.2f, 0.2f, 1.0f);\n        Color c = c1.lerp(c2, float(i) / 14.0f);\n        setColor(c.r, c.g, c.b);\n        drawRect(30 + float(i) * 16, 250, 14, 25);\n    }\n\n    // colorFromHSB\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"colorFromHSB\", 300, 230);\n    for (int i = 0; i < 15; i++) {\n        Color c = colorFromHSB(float(i) / 15.0f, 1.0f, 1.0f);\n        setColor(c.r, c.g, c.b);\n        drawRect(320 + float(i) * 16, 250, 14, 25);\n    }\n\n    // Rect\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"Rect.contains (hover test)\", 10, 310);\n    Rect r(50, 330, 150, 60);\n    bool inside = r.contains(getMouseX(), getMouseY());\n    if (inside) setColor(0.3f, 0.9f, 0.3f, 0.5f);\n    else setColor(0.5f, 0.5f, 0.5f, 0.3f);\n    drawRect(r.x, r.y, r.width, r.height);\n    setColor(1.0f, 0.5f, 0.0f);\n    drawCircle(r.getCenterX(), r.getCenterY(), 4);\n    setColor(1.0f, 1.0f, 1.0f, 0.6f);\n    drawBitmapString(\"contains: \" + (inside ? \"true\" : \"false\"), 220, 350);\n}\n",

    "4_easing": "// Sample 4: Easing & Tween\nfloat t4 = 0.0f;\nTween@[] tw(6);\nstring[] easeNames(6);\n\nvoid setupSample4() {\n    easeNames[0] = \"Linear\"; easeNames[1] = \"Quad\"; easeNames[2] = \"Cubic\";\n    easeNames[3] = \"Elastic\"; easeNames[4] = \"Bounce\"; easeNames[5] = \"Back\";\n    @tw[0] = createTween().from(0).to(300).duration(1.5f).ease(EaseType::Linear).start();\n    @tw[1] = createTween().from(0).to(300).duration(1.5f).ease(EaseType::Quad).start();\n    @tw[2] = createTween().from(0).to(300).duration(1.5f).ease(EaseType::Cubic).start();\n    @tw[3] = createTween().from(0).to(300).duration(1.5f).ease(EaseType::Elastic).start();\n    @tw[4] = createTween().from(0).to(300).duration(1.5f).ease(EaseType::Bounce).start();\n    @tw[5] = createTween().from(0).to(300).duration(1.5f).ease(EaseType::Back).start();\n}\n\nvoid drawSample4() {\n    t4 += getDeltaTime();\n    setColor(1.0f, 1.0f, 1.0f, 0.6f);\n    drawBitmapString(\"Easing & Tween: all EaseTypes, Tween chaining\", 10, 40);\n\n    // Tween bars\n    for (int i = 0; i < 6; i++) {\n        tw[i].update(getDeltaTime());\n        if (tw[i].isComplete()) tw[i].reset().start();\n        float y = 70 + float(i) * 40;\n        setColor(0.3f, 0.3f, 0.3f);\n        drawRect(120, y, 300, 25);\n        float hue = float(i) / 6.0f;\n        setColor(0.5f + 0.5f * sin(hue * TAU), 0.5f + 0.5f * sin(hue * TAU + 2.1f), 0.5f + 0.5f * sin(hue * TAU + 4.2f));\n        drawCircle(120 + tw[i].getValue(), y + 12, 10);\n        setColor(1.0f, 1.0f, 1.0f, 0.6f);\n        drawBitmapString(easeNames[i], 30, y + 8);\n    }\n\n    // Easing curves\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"Easing curves (In/Out/InOut):\", 10, 320);\n\n    float startX = 50;\n    float startY = 350;\n    float graphW = 80;\n    float graphH = 50;\n\n    // easeIn\n    setColor(0.2f, 0.2f, 0.2f);\n    drawRect(startX, startY, graphW, graphH);\n    setColor(0.9f, 0.4f, 0.4f);\n    for (int i = 0; i < 40; i++) {\n        float tt = float(i) / 40.0f;\n        drawPoint(startX + tt * graphW, startY + graphH - easeIn(tt, EaseType::Cubic) * graphH);\n    }\n    setColor(1.0f, 1.0f, 1.0f, 0.5f);\n    drawBitmapString(\"In\", startX + 30, startY + graphH + 8);\n\n    // easeOut\n    startX += 120;\n    setColor(0.2f, 0.2f, 0.2f);\n    drawRect(startX, startY, graphW, graphH);\n    setColor(0.4f, 0.9f, 0.4f);\n    for (int i = 0; i < 40; i++) {\n        float tt = float(i) / 40.0f;\n        drawPoint(startX + tt * graphW, startY + graphH - easeOut(tt, EaseType::Cubic) * graphH);\n    }\n    setColor(1.0f, 1.0f, 1.0f, 0.5f);\n    drawBitmapString(\"Out\", startX + 28, startY + graphH + 8);\n\n    // easeInOut\n    startX += 120;\n    setColor(0.2f, 0.2f, 0.2f);\n    drawRect(startX, startY, graphW, graphH);\n    setColor(0.4f, 0.4f, 0.9f);\n    for (int i = 0; i < 40; i++) {\n        float tt = float(i) / 40.0f;\n        drawPoint(startX + tt * graphW, startY + graphH - easeInOut(tt, EaseType::Cubic) * graphH);\n    }\n    setColor(1.0f, 1.0f, 1.0f, 0.5f);\n    drawBitmapString(\"InOut\", startX + 22, startY + graphH + 8);\n}\n",

    "5_texture": "// Sample 5: Texture (simulated)\nfloat t5 = 0.0f;\n\nvoid setupSample5() { }\n\nvoid drawSample5() {\n    t5 += getDeltaTime();\n    setColor(1.0f, 1.0f, 1.0f, 0.6f);\n    drawBitmapString(\"Texture API: loadTexture, draw, getWidth/Height\", 10, 40);\n    drawBitmapString(\"(Showing procedural patterns as texture placeholders)\", 10, 55);\n\n    // Checkerboard\n    float px = 50;\n    float py = 90;\n    for (int y = 0; y < 8; y++) {\n        for (int x = 0; x < 8; x++) {\n            if ((x + y) % 2 == 0) setColor(0.9f, 0.2f, 0.2f);\n            else setColor(0.2f, 0.2f, 0.9f);\n            drawRect(px + float(x) * 12, py + float(y) * 12, 11, 11);\n        }\n    }\n    setColor(1.0f, 1.0f, 1.0f, 0.5f);\n    drawBitmapString(\"Checker\", px, py + 105);\n\n    // Gradient\n    px = 180;\n    for (int y = 0; y < 48; y++) {\n        for (int x = 0; x < 48; x++) {\n            setColor(float(x) / 48.0f, float(y) / 48.0f, 0.5f);\n            drawPoint(px + float(x) * 2, py + float(y) * 2);\n        }\n    }\n    setColor(1.0f, 1.0f, 1.0f, 0.5f);\n    drawBitmapString(\"Gradient\", px + 20, py + 105);\n\n    // Animated wave\n    px = 310;\n    for (int y = 0; y < 24; y++) {\n        for (int x = 0; x < 24; x++) {\n            float w = sin(float(x) * 0.4f + t5 * 2.0f) * cos(float(y) * 0.4f + t5 * 1.5f);\n            float b = 0.5f + w * 0.5f;\n            setColor(b * 0.3f, b * 0.8f, b);\n            drawRect(px + float(x) * 4, py + float(y) * 4, 3, 3);\n        }\n    }\n    setColor(1.0f, 1.0f, 1.0f, 0.5f);\n    drawBitmapString(\"Animated\", px + 20, py + 105);\n\n    // Noise\n    px = 440;\n    for (int y = 0; y < 24; y++) {\n        for (int x = 0; x < 24; x++) {\n            float n = fract(sin(float(x) * 12.9898f + float(y) * 78.233f + t5 * 0.1f) * 43758.5453f);\n            setColor(n, n * 0.7f, n * 0.5f);\n            drawRect(px + float(x) * 4, py + float(y) * 4, 3, 3);\n        }\n    }\n    setColor(1.0f, 1.0f, 1.0f, 0.5f);\n    drawBitmapString(\"Noise\", px + 30, py + 105);\n\n    // Texture transforms\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"Texture transforms:\", 10, 250);\n\n    float baseY = 300;\n    // Original\n    setColor(0.8f, 0.6f, 0.2f);\n    drawRect(70, baseY, 50, 50);\n    setColor(1.0f, 1.0f, 1.0f);\n    drawBitmapString(\"T\", 90, baseY + 30);\n    setColor(1.0f, 1.0f, 1.0f, 0.5f);\n    drawBitmapString(\"Original\", 60, baseY + 60);\n\n    // Rotated\n    pushMatrix();\n    translate(220, baseY + 25);\n    rotate(t5);\n    setColor(0.8f, 0.6f, 0.2f);\n    drawRect(-25, -25, 50, 50);\n    setColor(1.0f, 1.0f, 1.0f);\n    drawBitmapString(\"T\", -5, 5);\n    popMatrix();\n    setColor(1.0f, 1.0f, 1.0f, 0.5f);\n    drawBitmapString(\"Rotated\", 195, baseY + 60);\n\n    // Scaled\n    pushMatrix();\n    translate(350, baseY + 25);\n    float sc = 0.6f + sin(t5 * 2.0f) * 0.3f;\n    scale(sc, sc);\n    setColor(0.8f, 0.6f, 0.2f);\n    drawRect(-25, -25, 50, 50);\n    setColor(1.0f, 1.0f, 1.0f);\n    drawBitmapString(\"T\", -5, 5);\n    popMatrix();\n    setColor(1.0f, 1.0f, 1.0f, 0.5f);\n    drawBitmapString(\"Scaled\", 330, baseY + 60);\n\n    // Tinted\n    pushMatrix();\n    translate(480, baseY + 25);\n    float h = fmod(t5 * 0.3f, 1.0f);\n    setColor(0.5f + 0.5f * sin(h * TAU), 0.5f + 0.5f * sin(h * TAU + 2.1f), 0.5f + 0.5f * sin(h * TAU + 4.2f));\n    drawRect(-25, -25, 50, 50);\n    setColor(1.0f, 1.0f, 1.0f);\n    drawBitmapString(\"T\", -5, 5);\n    popMatrix();\n    setColor(1.0f, 1.0f, 1.0f, 0.5f);\n    drawBitmapString(\"Tinted\", 460, baseY + 60);\n}\n",

    "6_sound": "// Sample 6: Sound (simulated visualizer)\nfloat t6 = 0.0f;\n\nvoid setupSample6() { }\n\nvoid drawSample6() {\n    t6 += getDeltaTime();\n    setColor(1.0f, 1.0f, 1.0f, 0.6f);\n    drawBitmapString(\"Sound API: loadSound, play, stop, pause, volume, pan, loop\", 10, 40);\n    drawBitmapString(\"(Showing simulated audio visualizers)\", 10, 55);\n\n    // Waveform\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"Waveform:\", 10, 85);\n    float waveY = 150;\n    setColor(0.15f, 0.15f, 0.2f);\n    drawRect(30, waveY - 50, 400, 100);\n    setColor(0.3f, 0.9f, 0.3f);\n    for (int i = 0; i < 199; i++) {\n        float x1 = 30 + float(i) * 2;\n        float x2 = 30 + float(i + 1) * 2;\n        float y1 = waveY + sin(float(i) * 0.1f + t6 * 3.0f) * 30 + sin(float(i) * 0.3f + t6 * 1.7f) * 15;\n        float y2 = waveY + sin(float(i + 1) * 0.1f + t6 * 3.0f) * 30 + sin(float(i + 2) * 0.3f + t6 * 1.7f) * 15;\n        drawLine(x1, y1, x2, y2);\n    }\n    setColor(0.5f, 0.5f, 0.5f, 0.3f);\n    drawLine(30, waveY, 430, waveY);\n\n    // Frequency bars\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"Frequency:\", 10, 230);\n    float barY = 320;\n    for (int i = 0; i < 24; i++) {\n        float freq = (sin(float(i) * 0.25f + t6 * 2.0f) * 0.5f + 0.5f) * (sin(float(i) * 0.5f - t6 * 0.8f) * 0.3f + 0.7f);\n        float h = freq * 60;\n        float hue = float(i) / 24.0f;\n        setColor(0.5f + 0.5f * sin(hue * TAU), 0.5f + 0.5f * sin(hue * TAU + 2.1f), 0.5f + 0.5f * sin(hue * TAU + 4.2f));\n        drawRect(30 + float(i) * 18, barY - h, 15, h);\n    }\n\n    // Controls\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"Controls:\", 480, 85);\n    // Play\n    setColor(0.3f, 0.9f, 0.3f);\n    drawTriangle(500, 110, 500, 135, 520, 122);\n    // Pause\n    setColor(0.9f, 0.9f, 0.3f);\n    drawRect(540, 110, 8, 25);\n    drawRect(552, 110, 8, 25);\n    // Stop\n    setColor(0.9f, 0.3f, 0.3f);\n    drawRect(580, 110, 25, 25);\n\n    // Volume\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"Vol:\", 480, 160);\n    setColor(0.3f, 0.3f, 0.3f);\n    drawRect(520, 165, 100, 8);\n    float vol = 0.5f + sin(t6 * 0.5f) * 0.3f;\n    setColor(0.3f, 0.6f, 0.9f);\n    drawRect(520, 165, vol * 100, 8);\n    drawCircle(520 + vol * 100, 169, 6);\n\n    // Pan\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"Pan:\", 480, 190);\n    setColor(0.3f, 0.3f, 0.3f);\n    drawRect(520, 195, 100, 8);\n    float pan = sin(t6 * 0.3f);\n    setColor(0.9f, 0.6f, 0.3f);\n    drawCircle(570 + pan * 50, 199, 6);\n}\n",

    "7_font": "// Sample 7: Font\nfloat t7 = 0.0f;\n\nvoid setupSample7() { }\n\nvoid drawSample7() {\n    t7 += getDeltaTime();\n    setColor(1.0f, 1.0f, 1.0f, 0.6f);\n    drawBitmapString(\"Font API: loadFont, draw, getStringWidth/Height\", 10, 40);\n    drawBitmapString(\"(Using built-in drawBitmapString)\", 10, 55);\n\n    // Basic text\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"Basic text:\", 10, 85);\n    setColor(1.0f, 1.0f, 1.0f);\n    drawBitmapString(\"The quick brown fox jumps over the lazy dog.\", 30, 105);\n\n    // Colored text\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"Colored:\", 10, 135);\n    setColor(0.9f, 0.3f, 0.3f);\n    drawBitmapString(\"Red\", 30, 155);\n    setColor(0.3f, 0.9f, 0.3f);\n    drawBitmapString(\"Green\", 80, 155);\n    setColor(0.3f, 0.3f, 0.9f);\n    drawBitmapString(\"Blue\", 150, 155);\n    float h = fmod(t7 * 0.3f, 1.0f);\n    setColor(0.5f + 0.5f * sin(h * TAU), 0.5f + 0.5f * sin(h * TAU + 2.1f), 0.5f + 0.5f * sin(h * TAU + 4.2f));\n    drawBitmapString(\"Rainbow!\", 210, 155);\n\n    // Numbers & symbols\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"Numbers & Symbols:\", 10, 185);\n    setColor(0.8f, 0.8f, 0.5f);\n    drawBitmapString(\"0123456789 !@#$%^&*()_+-=[]{}|;:',.<>?/\", 30, 205);\n\n    // Transforms\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"Transformed:\", 10, 240);\n\n    pushMatrix();\n    translate(80, 280);\n    rotate(sin(t7) * 0.2f);\n    setColor(0.9f, 0.6f, 0.2f);\n    drawBitmapString(\"Wobble\", -25, 0);\n    popMatrix();\n\n    pushMatrix();\n    translate(200, 280);\n    float s = 1.0f + sin(t7 * 2.0f) * 0.15f;\n    scale(s, s);\n    setColor(0.6f, 0.2f, 0.9f);\n    drawBitmapString(\"Pulse\", -20, 0);\n    popMatrix();\n\n    setColor(0.2f, 0.9f, 0.6f);\n    drawBitmapString(\"Slide\", 320 + sin(t7 * 1.5f) * 40, 280);\n\n    // Dynamic info\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"Dynamic:\", 10, 320);\n    setColor(0.8f, 0.8f, 0.8f);\n    drawBitmapString(\"Time: \" + toString(t7), 30, 340);\n    drawBitmapString(\"Mouse: \" + toString(int(getMouseX())) + \", \" + toString(int(getMouseY())), 30, 355);\n    drawBitmapString(\"Window: \" + toString(int(getWindowWidth())) + \"x\" + toString(int(getWindowHeight())), 30, 370);\n\n    // Typewriter\n    string full = \"This text appears letter by letter...\";\n    int chars = int(fmod(t7 * 8, float(full.length()) + 8));\n    if (chars > int(full.length())) chars = full.length();\n    setColor(0.5f, 0.9f, 0.5f);\n    drawBitmapString(full.substr(0, chars), 250, 340);\n    if (int(t7 * 2) % 2 == 0) drawBitmapString(\"_\", 250 + chars * 8, 340);\n}\n",

    "8_input": "// Sample 8: Input\nfloat t8 = 0.0f;\nVec2[] trail(30);\nint trailIdx = 0;\nint lastKey = 0;\nVec2 clickPos;\nint clickBtn = -1;\nfloat clickTime = -10.0f;\n\nvoid setupSample8() {\n    for (int i = 0; i < 30; i++) trail[i] = Vec2(-100, -100);\n}\n\nvoid drawSample8() {\n    t8 += getDeltaTime();\n    trailIdx = (trailIdx + 1) % 30;\n    trail[trailIdx] = Vec2(getMouseX(), getMouseY());\n\n    setColor(1.0f, 1.0f, 1.0f, 0.6f);\n    drawBitmapString(\"Input: getMouseX/Y, mousePressed, keyPressed - interact!\", 10, 40);\n\n    // Mouse trail\n    for (int i = 0; i < 29; i++) {\n        int i1 = (trailIdx - i + 30) % 30;\n        int i2 = (trailIdx - i - 1 + 30) % 30;\n        if (trail[i1].x > 0 && trail[i2].x > 0) {\n            float a = 1.0f - float(i) / 29.0f;\n            float hue = float(i) / 29.0f;\n            setColor(0.5f + 0.5f * sin(hue * TAU), 0.5f + 0.5f * sin(hue * TAU + 2.1f), 0.5f + 0.5f * sin(hue * TAU + 4.2f), a);\n            drawLine(trail[i1].x, trail[i1].y, trail[i2].x, trail[i2].y);\n        }\n    }\n\n    // Cursor\n    setColor(1.0f, 1.0f, 1.0f, 0.8f);\n    drawCircle(getMouseX(), getMouseY(), 8);\n    setColor(0.3f, 0.9f, 1.0f);\n    drawCircle(getMouseX(), getMouseY(), 5);\n\n    // Click ripple\n    if (t8 - clickTime < 0.4f) {\n        float r = (t8 - clickTime) * 80;\n        float a = 1.0f - (t8 - clickTime) * 2.5f;\n        if (clickBtn == 0) setColor(0.3f, 1.0f, 0.3f, a);\n        else if (clickBtn == 1) setColor(1.0f, 0.3f, 0.3f, a);\n        else setColor(0.3f, 0.3f, 1.0f, a);\n        drawCircle(clickPos.x, clickPos.y, r);\n    }\n\n    // Info\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"Mouse:\", 10, 80);\n    setColor(0.8f, 0.8f, 0.8f);\n    drawBitmapString(\"X: \" + toString(int(getMouseX())) + \"  Y: \" + toString(int(getMouseY())), 30, 100);\n    if (clickBtn >= 0) {\n        string btn = (clickBtn == 0 ? \"Left\" : (clickBtn == 1 ? \"Right\" : \"Middle\"));\n        drawBitmapString(\"Last click: \" + btn + \" at (\" + toString(int(clickPos.x)) + \",\" + toString(int(clickPos.y)) + \")\", 30, 115);\n    }\n\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"Keyboard:\", 10, 145);\n    setColor(0.8f, 0.8f, 0.8f);\n    drawBitmapString(\"Last key code: \" + toString(lastKey), 30, 165);\n\n    // Virtual keyboard\n    string[] rows = {\"1234567890\", \"QWERTYUIOP\", \"ASDFGHJKL\", \"ZXCVBNM\"};\n    float kx = 30;\n    float ky = 200;\n    float ks = 28;\n    for (int r = 0; r < 4; r++) {\n        for (int c = 0; c < int(rows[r].length()); c++) {\n            float x = kx + float(r) * 8 + float(c) * ks;\n            float y = ky + float(r) * ks;\n            int code = int(rows[r][c]);\n            bool active = (lastKey == code || lastKey == code + 32);\n            if (active) setColor(0.3f, 0.9f, 0.3f);\n            else setColor(0.25f, 0.25f, 0.3f);\n            drawRect(x, y, ks - 3, ks - 3);\n            if (active) setColor(0.0f, 0.0f, 0.0f);\n            else setColor(0.6f, 0.6f, 0.6f);\n            drawBitmapString(rows[r].substr(c, 1), x + 8, y + 8);\n        }\n    }\n    // Space\n    bool spaceActive = (lastKey == 32);\n    if (spaceActive) setColor(0.3f, 0.9f, 0.3f);\n    else setColor(0.25f, 0.25f, 0.3f);\n    drawRect(kx + 60, ky + 4 * ks, ks * 4, ks - 3);\n    if (spaceActive) setColor(0.0f, 0.0f, 0.0f);\n    else setColor(0.6f, 0.6f, 0.6f);\n    drawBitmapString(\"SPACE\", kx + 85, ky + 4 * ks + 8);\n}\n\nvoid keySample8(int key) { lastKey = key; }\nvoid mouseSample8(float x, float y, int btn) { clickPos = Vec2(x, y); clickBtn = btn; clickTime = t8; }\n",

    "9_style": "// Sample 9: Style\nfloat t9 = 0.0f;\n\nvoid setupSample9() { }\n\nvoid drawSample9() {\n    t9 += getDeltaTime();\n    setColor(1.0f, 1.0f, 1.0f, 0.6f);\n    drawBitmapString(\"Style: pushStyle/popStyle, getColor, stroke settings\", 10, 40);\n\n    // pushStyle/popStyle\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"pushStyle/popStyle:\", 10, 70);\n\n    setColor(0.9f, 0.3f, 0.3f);\n    drawRect(30, 90, 50, 50);\n    pushStyle();\n    setColor(0.3f, 0.9f, 0.3f);\n    drawRect(90, 90, 50, 50);\n    pushStyle();\n    setColor(0.3f, 0.3f, 0.9f);\n    drawRect(150, 90, 50, 50);\n    popStyle();\n    drawRect(210, 90, 50, 50);\n    popStyle();\n    drawRect(270, 90, 50, 50);\n\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"Red\", 35, 145);\n    drawBitmapString(\"Green\", 92, 145);\n    drawBitmapString(\"Blue\", 155, 145);\n    drawBitmapString(\"Pop1\", 215, 145);\n    drawBitmapString(\"Pop2\", 275, 145);\n\n    // getColor\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"getColor:\", 380, 70);\n    setColor(0.8f, 0.5f, 0.2f);\n    drawRect(400, 90, 50, 50);\n    Color c = getColor();\n    setColor(1.0f, 1.0f, 1.0f, 0.6f);\n    drawBitmapString(\"R:\" + toString(c.r), 400, 145);\n    drawBitmapString(\"G:\" + toString(c.g), 400, 158);\n    drawBitmapString(\"B:\" + toString(c.b), 400, 171);\n\n    // Stroke weight\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"setStrokeWeight:\", 10, 200);\n    float[] weights = {1, 3, 5, 8, 12};\n    for (int i = 0; i < 5; i++) {\n        setStrokeWeight(weights[i]);\n        setColor(0.9f, 0.6f, 0.2f);\n        beginStroke();\n        vertex(40 + float(i) * 80, 220);\n        vertex(80 + float(i) * 80, 260);\n        endStroke();\n        setColor(1.0f, 1.0f, 1.0f, 0.4f);\n        drawBitmapString(toString(weights[i]) + \"px\", 45 + float(i) * 80, 270);\n    }\n\n    // Stroke cap\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"setStrokeCap:\", 10, 300);\n    setStrokeWeight(10);\n    setStrokeCap(StrokeCap::Butt);\n    setColor(0.2f, 0.8f, 0.6f);\n    beginStroke(); vertex(40, 330); vertex(100, 330); endStroke();\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"Butt\", 55, 345);\n\n    setStrokeCap(StrokeCap::Round);\n    setColor(0.6f, 0.2f, 0.8f);\n    beginStroke(); vertex(150, 330); vertex(210, 330); endStroke();\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"Round\", 160, 345);\n\n    setStrokeCap(StrokeCap::Square);\n    setColor(0.8f, 0.6f, 0.2f);\n    beginStroke(); vertex(260, 330); vertex(320, 330); endStroke();\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"Square\", 268, 345);\n\n    // Stroke join\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"setStrokeJoin:\", 10, 375);\n    setStrokeWeight(8);\n    setStrokeCap(StrokeCap::Round);\n\n    setStrokeJoin(StrokeJoin::Miter);\n    setColor(0.9f, 0.3f, 0.3f);\n    beginStroke(); vertex(40, 430); vertex(70, 395); vertex(100, 430); endStroke();\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"Miter\", 55, 440);\n\n    setStrokeJoin(StrokeJoin::Round);\n    setColor(0.3f, 0.9f, 0.3f);\n    beginStroke(); vertex(150, 430); vertex(180, 395); vertex(210, 430); endStroke();\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"Round\", 160, 440);\n\n    setStrokeJoin(StrokeJoin::Bevel);\n    setColor(0.3f, 0.3f, 0.9f);\n    beginStroke(); vertex(260, 430); vertex(290, 395); vertex(320, 430); endStroke();\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"Bevel\", 270, 440);\n\n    setStrokeWeight(1);\n\n    // Alpha\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"Alpha blending:\", 380, 300);\n    setColor(0.9f, 0.2f, 0.2f);\n    drawCircle(440, 370, 40);\n    setColor(0.2f, 0.9f, 0.2f, 0.6f);\n    drawCircle(475, 370, 40);\n    setColor(0.2f, 0.2f, 0.9f, 0.4f);\n    drawCircle(457, 405, 40);\n}\n",

    "10_math": "// Sample 10: Math & Utility\nfloat t10 = 0.0f;\n\nvoid setupSample10() { }\n\nvoid drawSample10() {\n    t10 += getDeltaTime();\n    setColor(1.0f, 1.0f, 1.0f, 0.6f);\n    drawBitmapString(\"Math: lerp, map, clamp, fract, random, window functions\", 10, 40);\n\n    // lerp\n    float lv = lerp(0.0f, 200.0f, (sin(t10) + 1.0f) / 2.0f);\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"lerp(0,200,t): \" + toString(int(lv)), 10, 75);\n    setColor(0.3f, 0.9f, 0.3f);\n    drawRect(180, 70, lv, 15);\n\n    // map\n    float mv = map(sin(t10), -1.0f, 1.0f, 0.0f, 200.0f);\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"map(sin,-1,1,0,200): \" + toString(int(mv)), 10, 105);\n    setColor(0.9f, 0.3f, 0.3f);\n    drawRect(230, 100, mv, 15);\n\n    // clamp\n    float raw = sin(t10 * 2.0f) * 150.0f;\n    float cv = clamp(raw, 0.0f, 100.0f);\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"clamp(sin*150,0,100): \" + toString(int(cv)), 10, 135);\n    setColor(0.5f, 0.5f, 0.5f, 0.3f);\n    drawRect(240, 130, 100, 15);\n    setColor(0.3f, 0.3f, 0.9f);\n    drawRect(240, 130, cv, 15);\n\n    // fract\n    float fv = fract(t10);\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"fract(time): \" + toString(fv), 10, 165);\n    setColor(0.9f, 0.6f, 0.2f);\n    drawRect(160, 160, fv * 100, 15);\n\n    // random visualization\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"random (scatter):\", 10, 200);\n    for (int i = 0; i < 50; i++) {\n        float rx = 30 + fract(sin(float(i) * 43.23f + t10 * 0.1f) * 9823.34f) * 150;\n        float ry = 220 + fract(cos(float(i) * 23.45f + t10 * 0.1f) * 7654.12f) * 80;\n        setColor(0.8f, 0.8f, 0.2f, 0.7f);\n        drawCircle(rx, ry, 3);\n    }\n\n    // Window info\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"Window:\", 10, 320);\n    setColor(0.8f, 0.8f, 0.8f);\n    drawBitmapString(\"getWindowWidth: \" + toString(int(getWindowWidth())), 30, 340);\n    drawBitmapString(\"getWindowHeight: \" + toString(int(getWindowHeight())), 30, 355);\n    drawBitmapString(\"getDeltaTime: \" + toString(getDeltaTime()), 30, 370);\n    drawBitmapString(\"getElapsedTime: \" + toString(getElapsedTime()), 30, 385);\n\n    // Constants\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"Constants:\", 10, 415);\n    setColor(0.8f, 0.8f, 0.8f);\n    drawBitmapString(\"TAU = \" + toString(TAU), 30, 435);\n    drawBitmapString(\"HALF_TAU = \" + toString(HALF_TAU), 30, 450);\n    drawBitmapString(\"QUARTER_TAU = \" + toString(QUARTER_TAU), 30, 465);\n\n    // Clock\n    setColor(1.0f, 1.0f, 1.0f, 0.4f);\n    drawBitmapString(\"Clock:\", 400, 200);\n    float cx = 480;\n    float cy = 300;\n    float cr = 60;\n    setColor(0.2f, 0.2f, 0.25f);\n    drawCircle(cx, cy, cr);\n    // Ticks\n    for (int i = 0; i < 12; i++) {\n        float a = float(i) * TAU / 12.0f - QUARTER_TAU;\n        setColor(0.6f, 0.6f, 0.6f);\n        drawLine(cx + cos(a) * (cr - 8), cy + sin(a) * (cr - 8), cx + cos(a) * (cr - 3), cy + sin(a) * (cr - 3));\n    }\n    // Second\n    float sa = fract(t10) * TAU - QUARTER_TAU;\n    setColor(0.9f, 0.3f, 0.3f);\n    drawLine(cx, cy, cx + cos(sa) * (cr - 15), cy + sin(sa) * (cr - 15));\n    // Minute\n    float ma = fract(t10 / 60.0f) * TAU - QUARTER_TAU;\n    setColor(0.3f, 0.9f, 0.3f);\n    drawLine(cx, cy, cx + cos(ma) * (cr - 25), cy + sin(ma) * (cr - 25));\n    setColor(1.0f, 1.0f, 1.0f);\n    drawCircle(cx, cy, 4);\n}\n"
  }
}
